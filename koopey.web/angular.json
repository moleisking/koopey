{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "e95774da-467b-4102-a77a-3a817188dbbc",
    "cache": {      
      "enabled": true,
      "environment": "all"
    }
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "KoopeyWebpage": {
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "cn": "src/localization/cn.json",
          "de": "src/localization/de.json",
          "es": "src/localization/es.json",
          "fr": "src/localization/fr.json",
          "it": "src/localization/it.json",
          "pt": "src/localization/pt.json"
        }
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": ".storybook",
            "buildTarget": "KoopeyWebpage:build",
            "port": 6006,
            "compodoc": false
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": ".storybook",
            "buildTarget": "KoopeyWebpage:build",
            "outputDir": "dist/KoopeyStorybook",
            "compodoc": false
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "localize": ["en"],
            "outputPath": {
              "base": "dist/KoopeyWebpage"
            },
            "index": "src/index.html",          
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets", "src/images"],
            "styles": [
              "src/styles/global.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": [],
            "browser": "src/main.ts",
            "polyfills": ["zone.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "@environments/environment.ts",
                  "with": "@environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                }
              ]
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "./../../environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "./../../environments/environment.ts",
                  "with": "./../../environments/environment.test.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "25mb"
                }
              ],
               "polyfills": ["zone.js", "zone.js/testing"]
            },
            "cn": { "localize": ["cn"] },
            "de": { "localize": ["de"] },
            "en": { "localize": ["en"] },
            "es": { "localize": ["es"] },
            "fr": { "localize": ["fr"] },
            "it": { "localize": ["it"] },
            "pt": { "localize": ["pt"] }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "KoopeyWebpage:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "KoopeyWebpage:build:production"
            },
            "development": {
              "buildTarget": "KoopeyWebpage:build:development,en"
            },
            "development-es": {
              "buildTarget": "KoopeyWebpage:build:development,es"
            },   
            "development-de": {
              "buildTarget": "KoopeyWebpage:build:development,de"
            },           
            "test": {
              "buildTarget": "KoopeyWebpage:build:test"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets", "src/images"],
            "styles": [
              "src/styles/global.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }      
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
