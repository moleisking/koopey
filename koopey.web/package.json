  "scripts": {
    "start": "npm run localhost",
    "reinstall": "npm cache clean && rimraf dist typings node_modules && npm i",
    "delete": "rimraf dist",
    "build": "node ./src/app/config/renProd.js && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "serveprod": "start http://md-sk-ws-1:3000/ && webpack-dev-server --content-base dist/ --port 3000",
    "serveprod.old": "start http://localhost:3000/ && webpack-dev-server --content-base dist/ --port ",
    "server": "node ./src/app/config/renDev.js && start http://192.168.1.110:3000/ && webpack-dev-server --inline --progress --port 3000 --host 192.168.1.110",
    "laptop": "node ./src/app/config/renDev.js && start http://192.168.1.90:3000/ && webpack-dev-server --inline --progress --port 3000 --host 192.168.1.90",
    "localhost": "node ./src/app/config/renDev.js && start http://127.0.0.1:3000/ && webpack-dev-server --inline --progress --port 3000 --host 127.0.0.1",
    "desktop": "node ./src/app/config/renDev.js && start http://md-sk-ws-1:3000/ && webpack-dev-server --inline --progress --port 3000 --host md-sk-ws-1"
  },
