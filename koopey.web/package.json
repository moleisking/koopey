{
  "name": "koopey.web",
  "version": "0.0.1",
  "description": "Modern classifieds for a modern world",
  "author": "Scott Johnston",
  "licence": {
    "type": "OSL-3.0",
    "url": "https://opensource.org/licenses/OSL-3.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moleisking/koopey.git"
  },
  "bugs": {
    "url": "https://github.com/moleisking/koopey/issues"
  },
  "homepage": "https://github.com/moleisking/koopey#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=development",
    "start-dev-es": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=development-es",
    "start-dev-de": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=development-de",
    "start-test": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=test",
    "start-prod": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=production",
    "build": "ng build --configuration=production --localize",
    "test": "ng test",
    "lint": "ng lint",
    "localize": "ng extract-i18n --format=json --output-path src/localization",
    "e2e": "ng e2e",
    "reinstall": "npm cache clean && rimraf dist typings node_modules && npm i",
    "delete": "rimraf dist",
    "environment": "set NODE_ENV=dev && set NODE_OPTIONS=--openssl-legacy-provider",
    "storybook": "set NODE_ENV=dev && set NODE_OPTIONS=--openssl-legacy-provider && ng run KoopeyWebpage:storybook"
  },
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@types/ws": "^8.5.4",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "@zxing/ngx-scanner": "^20.0.0",
    "angular-calendar": "^0.28.22",
    "angularx-qrcode": "^20.0.0",
    "core-js": "^3.39.0",
    "date-fns": "^4.1.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ngx-image-cropper": "^9.0.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.3",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "20.3.1",
    "@angular/localize": "^20.3.1",
    "@storybook/angular": "^9.1.15",
    "@types/core-js": "^2.5.8",
    "@types/crypto-js": "^4.2.2",
    "@types/google.maps": "3.45.6",
    "@types/hammerjs": "^2.0.46",
    "@types/jasmine": "^3.6.9",
    "@types/node": "^22.10.5",
    "@types/request": "^2.48.12",
    "codelyzer": "^0.0.28",
    "crypto-js": "^4.2.0",
    "eslint": "^9.17.0",
    "jasmine-core": "^5.1.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "2.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2"
  }
}
