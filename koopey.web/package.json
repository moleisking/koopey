{
  "name": "koopey.web",
  "version": "0.0.1",
  "description": "Modern classifieds for a modern world",
  "author": "Scott Johnston",
  "licence": {
    "type": "OSL-3.0",
    "url": "https://opensource.org/licenses/OSL-3.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moleisking/koopey.git"
  },
  "bugs": {
    "url": "https://github.com/moleisking/koopey/issues"
  },
  "homepage": "https://github.com/moleisking/koopey#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startdev": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=development",
    "starttest": "set NODE_OPTIONS=--openssl-legacy-provider ng serve --configuration=test",
    "startprod": "set NODE_OPTIONS=--openssl-legacy-provider ng serve --configuration=production",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "reinstall": "npm cache clean && rimraf dist typings node_modules && npm i",
    "delete": "rimraf dist",
    "environment": "set NODE_ENV=dev && set NODE_OPTIONS=--openssl-legacy-provider"
  },
  "dependencies": {
    "@angular/animations": "12.2.6",
    "@angular/cdk": "12.2.6",
